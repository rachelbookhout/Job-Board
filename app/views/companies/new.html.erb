<%= form_for @company do |f| %>

<%= f.label :name %>
<%= f.text_field :name %>


<%= f.label :description %>
<%= f.text_field :description%>

<%= f.label :website %>
 <%= f.text_field :website %>

<%= f.label :location %>
<%= f.text_field :location, required: true %>

<%= f.submit "Create" %>
<% end %>


